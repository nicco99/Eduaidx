<body>
    <!-- Spinner Start -->
    <div id="spinner" class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
        <div class="spinner-border text-primary" style="width: 3rem; height: 3rem;" role="status">
            <span class="sr-only">Loading...</span>
        </div>
    </div>
    <!-- Spinner End -->
    <%- include('partials/header'); -%>

    <!-- Carousel Start -->
    <div class="container-fluid p-0 mb-5" >
        <div class="owl-carousel header-carousel position-relative" >
            <!-- Article Writing -->
            <div class="owl-carousel-item position-relative">
                <img style="max-height: 90vh;" class="img-fluid" src="img/carousel-1.jpg" alt="">
                <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(0, 0, 0, .4);">
                    <div class="container">
                        <div class="row justify-content-start">
                            <div class="col-10 col-lg-8">
                                <h5 class="text-white text-uppercase mb-3 animated slideInDown">Dissertation & Thesis Help</h5>
                                <h1 class="display-3 text-white animated slideInDown mb-4">Expert Guidance for Your Research Projects</h1>
                                <p class="fs-5 fw-medium text-white mb-4 pb-2">Benefit from professional help while preparing your dissertation, or research project.</p>
                                <a href="" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft">Learn More</a>
                                <a href="" class="btn btn-secondary py-md-3 px-md-5 animated slideInRight">Get Help Now</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            
    
            <!-- Classes and Course Management -->
            <div class="owl-carousel-item position-relative">
                <img style="max-height: 90vh;" class="img-fluid" src="img/carousel-2.jpg" alt="">
                <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(0, 0, 0, .4);">
                    <div class="container">
                        <div class="row justify-content-start">
                            <div class="col-10 col-lg-8">
                                <h5 class="text-white text-uppercase mb-3 animated slideInDown">Online Class & Course Management</h5>
                                <h1 class="display-3 text-white animated slideInDown mb-4">Have a helping hand with all your online classes</h1>
                                <p class="fs-5 fw-medium text-white mb-4 pb-2">Benefit from your own Virtual Assistant ready to help you with all your exams, assessments and group discussions.</p>
                                <a href="" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft">Learn More</a>
                                <a href="" class="btn btn-secondary py-md-3 px-md-5 animated slideInRight">Chat with a Tutor</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    
            <!-- Assignment Help -->
            <div class="owl-carousel-item position-relative">
                <img style="max-height: 90vh;" class="img-fluid" src="img/carousel-3.jpg" alt="">
                <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(0, 0, 0, .4);">
                    <div class="container">
                        <div class="row justify-content-start">
                            <div class="col-10 col-lg-8">
                                <h5 class="text-white text-uppercase mb-3 animated slideInDown">Assignment Help Services</h5>
                                <h1 class="display-3 text-white animated slideInDown mb-4">Your tutor is just one click away</h1>
                                <p class="fs-5 fw-medium text-white mb-4 pb-2">Consult one of our expert writers to expertly prepare, edit, or proofread your assignment before the due date.</p>
                                <a href="" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft">Learn More</a>
                                <a href="" class="btn btn-secondary py-md-3 px-md-5 animated slideInRight">Write my Paper</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    
            <!-- Website Development -->
            <div class="owl-carousel-item position-relative">
                <img style="max-height: 90vh;" class="img-fluid" src="img/carousel-4.jpg" alt="">
                <div class="position-absolute top-0 start-0 w-100 h-100 d-flex align-items-center" style="background: rgba(0, 0, 0, .4);">
                    <div class="container">
                        <div class="row justify-content-start">
                            <div class="col-10 col-lg-8">
                                <h5 class="text-white text-uppercase mb-3 animated slideInDown">Coding & Programming Homework Help</h5>
                                <h1 class="display-3 text-white animated slideInDown mb-4">Work with one of our current developers to create expert solutions for your class project</h1>
                                <p class="fs-5 fw-medium text-white mb-4 pb-2">Create web apps, SAAS, websites, AI, etc from a wide aray of languages such as (JavaScript | C Sharp | React | Node Js etc).</p>
                                <a href="" class="btn btn-primary py-md-3 px-md-5 me-3 animated slideInLeft">Request Sample Projects</a>
                                <a href="" class="btn btn-secondary py-md-3 px-md-5 animated slideInRight">Chat with a Developer</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            
        </div>
    </div>
    
    <!-- Carousel End -->


    <!-- Service Start -->
    <div class="container-xxl py-5">
        <%- include('component/assignmenttypes') %>
    </div>
    
    <!-- Service End -->


    <!-- About Start -->
    <%- include('component/about'); -%>
    <!-- About End -->


    <!-- Fact Start -->
    <%- include('component/numbers'); -%>
    <!-- Fact End -->


